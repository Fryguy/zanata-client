<?xml version="1.0" encoding="UTF-8" ?>

<xliff xmlns="urn:oasis:names:tc:xliff:document:1.1" xmlns:xyz="urn:appInfo:Items"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.1 http://www.oasis-open.org/committees/xliff/documents/xliff-core-1.1.xsd"
	version="1.1">
	<file source-language="en" datatype="plaintext" original="">
		<body>
			<!-- Generic column headings. The intent is that these can be reused across 
				multiple pages. -->
			<group>

				<trans-unit id="column.architecture">
					<source>Architecture</source>
					<context-group name="ctx">
						<context context-type="sourcefile">/rhn/profiles/PackageList</context>
					</context-group>
				</trans-unit>

				<trans-unit id="column.basechannel">
					<source>Base Channel</source>
					<context-group name="ctx">
						<context context-type="sourcefile">/rhn/profiles/List</context>
					</context-group>
				</trans-unit>

				<trans-unit id="column.created">
					<source>Created</source>
					<context-group name="ctx">
						<context context-type="sourcefile">/rhn/profiles/List</context>
					</context-group>
				</trans-unit>

				<trans-unit id="column.name">
					<source>Name</source>
					<context-group name="ctx">
						<context context-type="sourcefile">/rhn/profiles/List</context>
					</context-group>
				</trans-unit>

				<trans-unit id="column.package">
					<source>Package</source>
					<context-group name="ctx">
						<context context-type="sourcefile">/rhn/profiles/PackageList</context>
					</context-group>
				</trans-unit>

			</group>

			<!-- Generic row labels. The intent is that these can be reused across 
				multiple pages. -->
			<group>
				<context-group name="ctx">
					<context context-type="sourcefile">/rhn/profiles/Details</context>
				</context-group>

				<trans-unit id="row.name">
					<source>Name</source>
				</trans-unit>
				<trans-unit id="row.description">
					<source>Description</source>
				</trans-unit>
				<trans-unit id="row.ip">
					<source>IP Address</source>
				</trans-unit>
				<trans-unit id="row.hostname">
					<source>Hostname</source>
				</trans-unit>

				<trans-unit id="row.macaddress">
					<source>MAC Address</source>
				</trans-unit>
			</group>

			<trans-unit id="manysystems.message">
				<source>{0} systems</source>
				<context-group name="ctx">
					<context context-type="sourcefile">/rhn/configuration/ChannelOverview.do</context>
					<context context-type="paramnotes">"6 systems"</context>
				</context-group>
			</trans-unit>
			<trans-unit id="onesystem.message">
				<source>1 system</source>
				<context-group name="ctx">
					<context context-type="sourcefile">/rhn/configuration/ChannelOverview.do</context>
					<context context-type="paramnotes">"1 system"</context>
				</context-group>
			</trans-unit>

			<trans-unit id="nosystems.message">
				<source>No systems.</source>
				<context-group name="ctx">
					<context context-type="sourcefile">/rhn/errata/details/SystemsAffected</context>
					<context context-type="sourcefile">/rhn/users/SystemsAdmined</context>
					<context context-type="sourcefile">/rhn/systems/SystemList</context>
					<context context-type="sourcefile">/rhn/YourRhn</context>
				</context-group>
			</trans-unit>

		</body>
	</file>
</xliff>